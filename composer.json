{
  "name": "rootlocal/yii2-jstree",
  "description": "jsTree is jquery plugin for Yii2",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "jstree"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Alexander Zkharov",
      "email": "sys@eml.ru",
      "homepage": "https://rootlocal.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "yiisoft/yii2": "~2.0.45",
    "bower-asset/jstree": "~3",
    "bower-asset/jquery.hotkeys": "dev-master",
    "bower-asset/jquery.cookie": ">=1.4",
    "bower-asset/jstree-bootstrap-theme": "~1.0.2",
    "components/font-awesome": ">=5.0.6",
    "yiisoft/yii2-bootstrap": ">=2.0.0"
  },
  "suggest": {
    "yiisoft/yii2-coding-standards": "you can use this package to check for code style issues when contributing to repository"
  },
  "autoload": {
    "psr-4": {
      "rootlocal\\widgets\\jstree\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
